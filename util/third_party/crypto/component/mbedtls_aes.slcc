id: mbedtls_aes
package: platform
label: Mbed TLS Support for AES
description: >-
  This component provides support for the AES block cipher using Mbed TLS.
category: Platform|Security
quality: production
source:
  - path: util/third_party/crypto/mbedtls/library/aes.c
include:
  - path: util/third_party/crypto/mbedtls/include
    file_list:
      - path: mbedtls/aes.h
provides:
  - name: mbedtls_aes
  - name: mbedtls_block_cipher
    allow_multiple: true
requires:
  - name: mbedtls_core
  - name: mbedtls_random
template_contribution:
  - name: mbedtls_config
    value: MBEDTLS_AES_C
  - name: mbedtls_config
    value: MBEDTLS_CIPHER_MODE_CTR
  - name: mbedtls_config
    value: MBEDTLS_CIPHER_MODE_CBC
  - name: mbedtls_config
    value: MBEDTLS_CIPHER_MODE_CFB
documentation:
  docset: mbed-tls
  document: group-sl-crypto-aes
